{
  "expo": {
    "name": "ShopMunimApp",
    "slug": "ShopMunimApp",
    "version": "1.0.0",
    "cli": {
      "version": ">= 7.0.0",
      "appVersionSource": "remote"
    },
    "orientation": "portrait",
    "icon": "./assets/icon-v3.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon-v3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-v3.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.krutik3011.ShopMunimApp",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "adjustPan"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0f15d0e5-177c-4ce0-88e0-ee26c4b0bc10"
      }
    }
  }
}