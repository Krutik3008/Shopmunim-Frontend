{
    "cli": {
        "version": ">= 7.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EAS_BUILD_NPM_INSTALL_FLAGS": "--legacy-peer-deps",
                "npm_config_legacy_peer_deps": "true"
            }
        },
        "production": {
            "env": {
                "EAS_BUILD_NPM_INSTALL_FLAGS": "--legacy-peer-deps",
                "npm_config_legacy_peer_deps": "true"
            }
        }
    },
    "submit": {
        "production": {}
    }
}